<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude cache and temp directories from cloud backup -->
        <exclude domain="cache" />
        <exclude domain="no_backup" />
    </cloud-backup>
    <device-transfer>
        <!-- Exclude cache and temp directories from device transfer -->
        <exclude domain="cache" />
        <exclude domain="no_backup" />
    </device-transfer>
</data-extraction-rules>